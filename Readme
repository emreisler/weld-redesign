//Install Qt designer

sudo apt-get install qttools5-dev-tools
sudo apt-get install qttools5-dev

//start designer

cd /usr/lib/x86_64-linux-gnu/qt5/bin/
./designer


//convert to py file
pyuic5 <file.ui> -o <file.py>


//git commands 

